# Environment Variables for User Behavior Tracking API

# Server Configuration
PORT=3001
NODE_ENV=development

# Database Configuration (when implementing real database)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=tracking_db
# DB_USER=tracking_user
# DB_PASSWORD=your_password

# Cassandra Configuration
CASSANDRA_HOSTS=127.0.0.1
CASSANDRA_KEYSPACE=user_logs
CASSANDRA_LOCAL_DATA_CENTER=datacenter1

# Security
# JWT_SECRET=your_jwt_secret_here
# ENCRYPTION_KEY=your_encryption_key_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=1000

# CORS Configuration
ALLOWED_ORIGINS=https://nhuthibeauty.com,http://localhost:3000,http://localhost:3001

# Logging
LOG_LEVEL=info